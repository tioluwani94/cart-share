{
  "expo": {
    "name": "CartShare",
    "slug": "cartshare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#FAFAFA"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cartshare.app",
      "appleTeamId": "W6SZ22J8C2",
      "infoPlist": {
        "NSCameraUsageDescription": "CartShare needs camera access to scan your receipts and track spending."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FAFAFA"
      },
      "package": "com.cartshare.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "CartShare needs camera access to scan your receipts and track spending."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "cartshare"
  }
}
